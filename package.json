    "@types/react-dom": "^17.0.9",
    "axios": "^0.21.1",
    "html-parse-stringify": "^3.0.1",
    "i18next": "^21.6.6",
    "i18next-browser-languagedetector": "^6.1.2",
    "i18next-chained-backend": "^3.0.2",
    "i18next-http-backend": "^1.3.1",
    "i18next-resources-to-backend": "^1.0.0",
    "js-base64": "^3.6.1",
    "jsqr": "^1.4.0",
